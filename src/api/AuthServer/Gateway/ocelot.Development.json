{
  "Routes": [
    {
      "ServiceName": "AuctionService",
      "DownstreamPathTemplate": "/api/auction",
      "DownstreamScheme": "https",
      "UpstreamPathTemplate": "/auction",
      "UpstreamHttpMethod": []
    },
    {
      "ServiceName": "AuctionService",
      "DownstreamPathTemplate": "/messaging-auction",
      "DownstreamScheme": "wss",
      "UpstreamPathTemplate": "/messaging-auction",
      "UpstreamHttpMethod": []
    },
    {
      "ServiceName": "AuctionService",
      "DownstreamPathTemplate": "/messaging-auction/{path}",
      "DownstreamScheme": "wss",
      "UpstreamPathTemplate": "/messaging-auction/{path}",
      "UpstreamHttpMethod": []
    },
    {
      "ServiceName": "IdentityServer",
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "UpstreamPathTemplate": "/{url}",
      "UpstreamHttpMethod": []
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5152/api",
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500,
      "Type": "PollConsul",
      "PollingInterval": 100
    }
  }
}