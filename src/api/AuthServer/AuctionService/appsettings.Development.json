{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [ { "Name": "Console" } ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Discovery": {
    "ServiceName": "AuctionService",
    "ServiceId": "AuctionService",
    "Url": "http://localhost:5280"
  },
  "Consul": {
    "Url": "http://localhost:8500"
  }
}
