{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [ { "Name": "Console" } ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Authentication" : {
    "Issuer": "https://localhost:5001",
    "SecurityKey": "TheMostSecretKeyICouldImagine"
  },
  "Discovery": {
    "ServiceName": "AuctionService",
    "ServiceId": "AuctionService",
    "Url": "https://localhost:7130"
  },
  "Consul": {
    "Url": "http://localhost:8500"
  },
  "BlobServiceAccountConnectionString": "#{USER_SECRETS}#",
  "Kafka": {
    "BootstrapServers": "localhost:9092"
  },
  "AuctionMonitor": {
    "MonitoringIntervalSeconds": 120
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=host.docker.internal,1433;Initial Catalog=AuctionService;User ID=sa;Password=Pass1234Qq;Trust Server Certificate=true"
  }
}
